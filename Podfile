# Uncomment the next line to define a global platform for your project
platform :osx, '10.10'

target 'Fireshot' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'FirebaseCore', :path => './firebase_sdk/'
  pod 'FirebaseDatabase', :path => './firebase_sdk/'
  pod 'FirebaseStorage', :path => './firebase_sdk/'
  pod 'FirebaseAuth', :path => './firebase_sdk/'
  # Pods for Fireshot

end
